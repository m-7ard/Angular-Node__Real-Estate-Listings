{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ES2020",
        "baseUrl": "./src",
        "paths": {
            "@apiModels/*": ["api/models/*"]
        },
        "noImplicitAny": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "outDir": "dist",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "rootDirs": ["./src"]
    },
    "include": ["src/**/*", "babel.config.js", "static"]
}
